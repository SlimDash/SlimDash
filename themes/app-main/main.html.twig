[% extends '@theme/shared/base.html.twig' %]
[% block title %]Home[% endblock %]

[% block body_params %]class="hold-transition login-page"[% endblock %]
[%block header %]
 
[% endblock %]
[% block body %]

    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        [[ "company name" ]]
        <small>[[ "home" ]]</small>
      </h1>
      <ol class="breadcrumb">
        <li class="active"><a href="/"><i class="fa fa-home"></i>[[ "HOME" ]]</a></li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">

      <div class="row" id="projects">
        <div class="col-xs-12">
          <div class="box">
            <div class="box-header">
              <h3 class="box-title">To manage a project, select from the list below</h3>

              <div class="box-tools pull-right">
                <div class="btn-group">
                  <button type="button" class="btn btn-box-tool">
                    New Project</button>
                </div>
              </div>
            </div>
            <!-- /.box-header -->
            <div class="box-body table-responsive no-padding">
              <table class="table table-hover">
                <thead><tr>
                  <th>project</th>
                  <th>url</th>
                  <th>action</th>
                </tr>
	            </thead>
	            <tbody>
                	<tr v-for="(p, key, index) in projects">
                		<td>{{key}}</td>
                		<td>{{p.url}}</td>
                		<td><a :href="'/project/' + key">manage</a></td>
                	</tr>
              	</tbody>
          	</table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
      </div>

    </section>
    <!-- /.content -->

[% endblock %]

[% block footer %]
<script>
var example1 = new Vue({
  el: '#projects',
  data: {
    projects: [[projects|raw]]
  }
});
</script>
[% endblock %]
